{
    "name": "@kilig/backend",
    "version": "1.0.0",
    "description": "Kilig Agent Orchestration Backend",
    "main": "dist/index.js",
    "type": "module",
    "bin": {
        "kilig-cli": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsx src/index.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:pipeline": "tsx scripts/test_pipeline.ts",
        "test:skills": "tsx scripts/test_skills_integration.ts",
        "results:sync": "tsx scripts/sync_results_to_supabase.ts",
        "test:e2e": "pnpm run test:pipeline && pnpm run link:latest && pnpm run artifacts:normalize && pnpm run results:sync",
        "link:latest": "tsx scripts/link_latest_artifacts.ts",
        "artifacts:normalize": "tsx scripts/normalize_artifacts.ts",
        "cache:clear": "tsx scripts/clear-cache.ts"
    },
    "dependencies": {
        "@fastify/cors": "^11.2.0",
        "@fastify/helmet": "^13.0.2",
        "@google/adk": "^0.2.1",
        "@google/adk-devtools": "^0.2.1",
        "@google/genai": "^1.34.0",
        "@kilig/shared": "workspace:*",
        "@opensearch-project/opensearch": "^3.5.1",
        "@supabase/supabase-js": "^2.89.0",
        "chalk": "^5.3.0",
        "commander": "^14.0.2",
        "dotenv": "^16.6.1",
        "fastify": "^5.6.2",
        "langfuse": "^3.38.6",
        "ora": "^8.0.1",
        "redis": "^5.10.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "@vitest/coverage-v8": "^4.0.16",
        "tsx": "^4.21.0",
        "typescript": "^5.4.5",
        "vitest": "^4.0.16"
    }
}